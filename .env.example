# API Configuration
# Backend URL - Change port here to match your backend server
VITE_BACKEND_URL=http://127.0.0.1:8001
VITE_API_BASE_URL=http://127.0.0.1:8001/api/v1

# Application Configuration
VITE_ENV=development
VITE_VERSION=1.0.0
VITE_APP_NAME=User Management System

# Feature Flags
VITE_ENABLE_GDPR=true
VITE_ENABLE_BULK_OPERATIONS=true
VITE_ENABLE_AUDIT_LOGS=true
VITE_ENABLE_HEALTH_CHECK=true

# Development Settings
VITE_ENABLE_DEBUG=true
VITE_LOG_LEVEL=info

# Security
VITE_TOKEN_REFRESH_ENABLED=true
VITE_SESSION_TIMEOUT=3600

# UI Configuration
VITE_ITEMS_PER_PAGE=10
VITE_MAX_FILE_SIZE=5242880
VITE_SUPPORTED_FILE_TYPES=.csv,.json

# External Services (Optional)
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# API Configuration
REACT_APP_API_BASE_URL=https://api.yourdomain.com/api/v1

# Application Configuration
REACT_APP_ENV=development
REACT_APP_VERSION=1.0.0
REACT_APP_APP_NAME=User Management System

# Feature Flags
REACT_APP_ENABLE_GDPR=true
REACT_APP_ENABLE_BULK_OPERATIONS=true
REACT_APP_ENABLE_AUDIT_LOGS=true
REACT_APP_ENABLE_HEALTH_CHECK=true

# Development Settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info

# Security
REACT_APP_TOKEN_REFRESH_ENABLED=true
REACT_APP_SESSION_TIMEOUT=3600

# UI Configuration
REACT_APP_ITEMS_PER_PAGE=10
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_SUPPORTED_FILE_TYPES=.csv,.json

# External Services (Optional)
REACT_APP_ANALYTICS_ID=
REACT_APP_SENTRY_DSN=

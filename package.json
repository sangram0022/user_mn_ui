{
  "name": "user-management-ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "crypto-js": "^4.2.0",
    "lucide-react": "^0.545.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "web-vitals": "^5.1.0",
    "zustand": "^5.0.8"
  },
  "scripts": {
    "dev": "vite --mode development",
    "start": "vite",
    "build": "tsc && vite build --mode production",
    "build:dev": "tsc && vite build --mode development",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "validate": "node scripts/validate-imports.mjs",
    "prebuild": "npm run validate",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "analyze": "ANALYZE=true vite build",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "playwright test e2e/visual-regression.spec.ts",
    "test:visual:ui": "playwright test e2e/visual-regression.spec.ts --ui",
    "test:visual:update": "playwright test e2e/visual-regression.spec.ts --update-snapshots",
    "test:visual:debug": "playwright test e2e/visual-regression.spec.ts --debug",
    "test:visual:storybook": "playwright test e2e/visual-storybook.spec.ts",
    "test:visual:storybook:update": "playwright test e2e/visual-storybook.spec.ts --update-snapshots",
    "test:visual:storybook:ui": "playwright test e2e/visual-storybook.spec.ts --ui",
    "test:visual:all": "playwright test e2e/visual-*.spec.ts",
    "test:visual:all:update": "playwright test e2e/visual-*.spec.ts --update-snapshots"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.15.0",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/crypto-js": "^4.2.2",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.15.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-storybook": "^9.1.10",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.4",
    "msw": "^2.11.5",
    "playwright": "^1.56.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.4",
    "storybook": "^9.1.10",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^6.3.7",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}

{
  "name": "usermn",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "build:dev": "tsc -b && vite build --mode development",
    "build:prod": "NODE_ENV=production tsc -b && vite build --mode production",
    "optimize-build": "npm run analyze-bundle && npm run compress-assets",
    "preview": "vite preview --host 0.0.0.0",
    "serve": "vite preview --port 3000",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:unit": "vitest run --reporter=verbose --reporter=junit",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "vitest run --coverage",
    "test:performance": "lighthouse-ci autorun",
    "test:visual": "playwright test --grep @visual",
    "coverage:check": "nyc check-coverage --lines 80 --functions 80 --branches 80 --statements 80",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:ci": "eslint . --ext .ts,.tsx --format junit --output-file reports/lint-report.xml",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "analyze-bundle": "vite-bundle-analyzer dist",
    "compress-assets": "echo 'Compression will be handled by Vite plugins'",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:scan": "snyk test",
    "security:monitor": "snyk monitor",
    "docker:build": "docker build -t usermn:latest .",
    "docker:build:prod": "docker build --target production -t usermn:prod .",
    "docker:run": "docker run -p 8080:8080 usermn:prod",
    "docker:scan": "docker run --rm -v /var/run/docker.sock:/var/run/docker.sock aquasec/trivy image usermn:latest",
    "start:prod": "npm run build:prod && npm run serve",
    "start:docker": "docker-compose up --build",
    "start:docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "clean": "rm -rf dist coverage reports node_modules/.cache",
    "clean:all": "npm run clean && rm -rf node_modules",
    "reset": "npm run clean:all && npm install",
    "precommit": "npm run lint && npm run type-check && npm run test:run",
    "prepush": "npm run test:coverage && npm run security:audit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.59.20",
    "@tanstack/react-query-devtools": "^5.59.20",
    "axios": "^1.7.9",
    "dompurify": "^3.3.0",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.460.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.66.0",
    "react-i18next": "^15.1.3",
    "react-router-dom": "^7.1.3",
    "react-window": "^2.2.2",
    "recharts": "^3.3.0",
    "vite-plugin-pwa": "^1.1.0",
    "workbox-window": "^7.3.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/cli": "^4.0.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/vite": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-window": "^1.8.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.1.2",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.10",
    "husky": "^8.0.3",
    "jsdom": "^27.0.1",
    "lighthouse-ci": "^1.2.0",
    "msw": "^2.11.6",
    "nyc": "^15.1.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.49",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "snyk": "^1.1268.0",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^6.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.6"
  }
}
